    <link rel="stylesheet" href="/sections.css" />
  </head>
  <body>
    <p style="display: none" class="page">faculty</p>
    <%- include("partials/header") %> <%- include("partials/main") %>
    <div class="container">
      <div>
        <p class="room-name"></p>
        <table class="sched-table">
          <thead>
            <tr>
              <th>INSTRUCTOR</th>
              <th>SUBJECT</th>
            </tr>
          </thead>
          <tbody>
            <% for ( const ObjectKey in facultyarrobj ){ %>
            <% facultyarrobj[ObjectKey].forEach(dat => { %>
            <tr>
              <td><%= dat.prof %></td>
              <td><%= dat.subject %></td>
            </tr>

            <%}) %>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>
  </body>
